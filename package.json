{
  "name": "dice-roller",
  "version": "0.0.0",
  "description": "Actually useful dice roller",
  "main": "src/index.tsx",
  "scripts": {
    "precommit": "yarn lint && yarn validate",
    "lint": "yarn prettier -l '**/*.{ts,tsx,json}' && yarn eslint ./src",
    "validate": " tsc -p ./tsconfig.json",
    "build": "webpack --config webpack.config.js --env prod",
    "dev": "webpack-dev-server --hot --inline"
  },
  "keywords": [],
  "author": "Duncan Walter",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "tabWidth": 2
  },
  "dependencies": {
    "classnames": "2.2.6",
    "history": "4.9.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "typestyle": "2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.3.3",
    "@babel/plugin-proposal-class-properties": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@typescript-eslint/parser": "5.27.0",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@types/classnames": "2.2.7",
    "@types/history": "4.7.2",
    "@types/jest": "23.3.5",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "babel-loader": "8.0.4",
    "compression-webpack-plugin": "2.0.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "0.0.0-experimental-7a5b8227c-20220602",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.1.3",
    "mini-css-extract-plugin": "0.5.0",
    "prettier": "2.6.2",
    "tslib": "1.9.3",
    "typescript": "4.5.5",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.1.14",
    "webpack-visualizer-plugin": "0.1.11"
  }
}
